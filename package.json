{
  "name": "css_test_app_trillo",
  "version": "0.0.1",
  "private": true,
  "description": "css test app",
  "main": "index.js",
  "scripts": {
    "watch:sass": "node-sass sass/main.scss HTML/css/style.css -w",
    "start": "node index",
    "devserver": "npm-run-all --parallel start watch:sass",
    "compile:sass": "node-sass sass/main.scss HTML/css/style.comp.css",
    "prefix:css": "postcss --use autoprefixer -b 'last 5 versions' HTML/css/style.comp.css -o HTML/css/style.prefix.css",
    "compress:css": "node-sass HTML/css/style.prefix.css HTML/css/style.css --output-style compressed",
    "build:css": "npm-run-all compile:sass prefix:css compress:css"
  },
  "repository": {
    "type": "git",
    "url": "https://github.ibm.com/rddill/css-ux"
  },
  "keywords": [
    "css",
    "test app"
  ],
  "author": "Bob Dill, IBM Distinguished Engineer",
  "license": "Apache-2.0",
  "dependencies": {
    "body-parser": "^1.19.0",
    "cfenv": "^1.2.2",
    "cookie-parser": "^1.4.5",
    "ejs": "^3.0.2",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "https": "^1.0.0",
    "mime": "^2.4.4",
    "path": "^0.12.7",
    "redis": "^3.0.2",
    "request": "^2.88.2",
    "url": "^0.11.0"
  },
  "engines": {
    "node": "10.x",
    "npm": "6.x"
  },
  "devDependencies": {
    "autoprefixer": "^9.8.0",
    "concat": "1.0.3",
    "node-sass": "^4.14.1",
    "npm-run-all": "4.1.5",
    "postcss-cli": "^7.1.1"
  }
}
